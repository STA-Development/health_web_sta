{
  "name": "fhhealth-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clear": "rm -rf .next && rm -rf dist",
    "build": "npm run clear && next build && npm run copy:assets",
    "start": "next start -p 8080",
    "copy:assets": "mkdir dist && cp -R .next dist/.next && cp -R public dist/public && cp package.json dist && cp yarn.lock dist && cp app.yaml dist",
    "lint": "eslint ./ --ext js,jsx,ts,tsx",
    "lint-fix": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@sentry/nextjs": "^6.16.1",
    "@types/react-input-mask": "^3.0.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.21.2",
    "cypress": "^9.2.0",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "firebase": "^9.6.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "next": "12.0.7",
    "quickblox": "^2.13.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-input-mask": "^2.0.4",
    "react-use": "^17.3.2",
    "react-verification-code-input": "^1.2.9",
    "recaptcha-v3": "^1.9.1",
    "sass": "^1.46.0"
  },
  "devDependencies": {
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.4.1",
    "typescript": "4.5.4"
  }
}
