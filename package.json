{
  "name": "fhhealth-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clear": "rm -rf .next && rm -rf dist",
    "build": "npm run clear && next build && npm run copy:assets",
    "start": "next start -p 8080",
    "copy:assets": "mkdir dist && cp -R .next dist/.next && cp -R public dist/public && cp package.json dist && cp yarn.lock dist && cp app.yaml dist",
    "lint": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@types/react-input-mask": "^3.0.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.21.2",
    "cypress": "^9.1.1",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "firebase": "^8.4.3",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "next": "11.0.1",
    "quickblox": "^2.13.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-input-mask": "^2.0.4",
    "react-use": "^17.3.1",
    "react-verification-code-input": "^1.2.9",
    "recaptcha-v3": "^1.9.1",
    "sass": "^1.44.0"
  },
  "devDependencies": {
    "@types/react": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "prettier": "^2.4.1",
    "typescript": "4.3.5"
  }
}
