{
  "name": "fhhealth-web",
  "version": "1.21.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clear": "rm -rf .next && rm -rf dist",
    "build": "npm run clear && next build && npm run copy:assets",
    "start": "next start -p 8080",
    "copy:assets": "mkdir dist && cp -R .next dist/.next && cp -R public dist/public && cp package.json dist && cp yarn.lock dist && cp app.yaml dist",
    "lint": "eslint ./ --ext js,jsx,ts,tsx",
    "lint-fix": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "cypress:init": "cypress install",
    "cypress:open": "cypress open",
    "cypress": "cypress run",
    "cypress:record": "cypress run --record",
    "cypress:ci": "NODE_ENV=test start-server-and-test dev http://localhost:3000 cypress:record",
    "cypress:ci-record-all": "NODE_ENV=test start-server-and-test dev http://localhost:3000 cypress:record-all-video"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "@sentry/nextjs": "^6.18.1",
    "@types/react-redux": "^7.1.23",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.26.0",
    "chai": "^4.3.6",
    "cy-verify-downloads": "^0.0.7",
    "cypress": "^9.5.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "firebase": "^9.6.7",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "next": "12.1.0",
    "quickblox": "^2.13.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-phone-input-2": "^2.15.0",
    "react-redux": "^7.2.6",
    "react-use": "^17.3.2",
    "react-verification-code-input": "^1.2.9",
    "recaptcha-v3": "^1.9.1",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.6.2"
  }
}
